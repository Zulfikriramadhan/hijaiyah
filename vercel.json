{
    "version": 2,
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.py" // <--- INI PERBAIKANNYA: ganti "use" menjadi "dest"
        }
    ]
}
